    "ts-node-dev": "^2.0.0",
    "vite": "^5.0.0",
    "concurrently": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.1",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7"
  }
}
